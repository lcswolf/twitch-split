{
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "import-name": false,
    "function-name": false,
    "curly": [true, "ignore-same-line"],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "interface-name": [true, "never-prefix"],
    "prettier": true
  },
  "jsRules": true
}
